<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Authors>Dennis Geller</Authors>
    <Description>Candlestick chart control for WPF.

Recompiled for .NET 5 by Nathaniel Moschkin</Description>
    <Product>FancyCandles</Product>
    <PackageId>FancyCandles</PackageId>
    <Version>1.1.0</Version>
    <Copyright>Copyright ©  2019</Copyright>
    <RootNamespace>FancyCandles</RootNamespace>
    <AssemblyName>FancyCandles</AssemblyName>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="CandleChart.xaml.cs">
      <DependentUpon>CandleChart.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="CandleChart.xaml">
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
